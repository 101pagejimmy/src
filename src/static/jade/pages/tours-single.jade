              .cell-xs-6.cell-sm-5.cell-md-3
                div
                  p.text-extra-small.text-silver-chalice.text-italic.text-uppercase.text-spacing-200 Date
                .offset-top-0
                  p.text-big.text-ubold.text-gray-base.text-uppercase 19 oct - 29 oct

              .cell-xs-6.cell-sm-5.cell-md-3.offset-top-40.offset-xs-top-0
                div
                  p.text-extra-small.text-silver-chalice.text-italic.text-uppercase.text-spacing-200 DURATION
                .offset-top-0
                  p.text-big.text-ubold.text-gray-base.text-uppercase 10 Days

              .cell-xs-6.cell-sm-5.cell-md-3.offset-top-40.offset-md-top-0
                div
                  p.text-extra-small.text-silver-chalice.text-italic.text-uppercase.text-spacing-200 PRICE
                .offset-top-0
                  p.text-big.text-ubold.text-gray-base.text-uppercase $2,500

              .cell-xs-6.cell-sm-5.cell-md-3.offset-top-40.offset-md-top-0
