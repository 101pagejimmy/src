extends  ../_skeleton

block title
  title SunTravel

block page
  // Page Header
  header.page-header.slider-menu-position
    +rd-navbar-default({
      "style": "rd-navbar-transparent rd-navbar-dark-stuck",
      "logo": '<img width="148" height="30" src="images/logo-light-148x30.png" alt="">',
      "logoFixed": '<img width="148" height="30" src="images/logo-dark-148x30.png" alt="">',
      "activeLink": 1,
      "attr": {
        "data-layout": "rd-navbar-fixed",
        "data-sm-layout": "rd-navbar-fixed",
        "data-md-layout": "rd-navbar-fullwidth",
        "data-lg-layout": "rd-navbar-static"
      }
    })

    // Swiper
    .swiper-container.swiper-slider(data-height='', data-min-height='400px', data-simulate-touch="false" data-slide-effect="fade")
      .swiper-wrapper
        .swiper-slide(data-slide-bg='images/backgrounds/background-01-1920x900.jpg')
        .swiper-slide(data-slide-bg='images/backgrounds/background-02-1920x900.jpg')
        .swiper-slide(data-slide-bg='images/backgrounds/background-03-1920x900.jpg')

      .swiper-caption-absolute
        .shell
          .range.range-xs-center
            .cell-lg-10
              div
                h1.text-white Enjoy Your Dream Vacation
              .offset-top-8
                p.h6.text-white Travel to the any corner of the world, without going around in circles.
              .offset-top-30.offset-md-top-60.inset-left-20.inset-right-20.inset-sm-left-0.inset-sm-right-0
                // Form
                form.rd-mailform.form-inline-search
                  .form-group.form-group-xs.form-inline-item
                    label.form-label(for="index-destination") Your Destination
                    input#index-destination.form-control(type="text" name="destination")
                  .form-group.form-group-xs.form-inline-item.form-inline-item-xs
                    label.form-label(for="index-arrival") Arrival
                    input#index-arrival.form-control(type="text" name="destination")
                  .form-group.form-group-xs.form-inline-item.form-inline-item-xs
                    label.form-label(for="index-departure") Departure
                    input#index-departure.form-control(type="text" name="departure")
                  .form-group.form-group-xs.form-inline-item
                    label.form-label(for="index-budget") Your Budget ($)
                    input#index-budget.form-control(type="text" name="budget")
                  .form-inline-item.offset-top-15.offset-sm-top-0
                    button(type="submit").btn.btn-primary Search


      // Swiper Pagination
      .swiper-pagination
      // Swiper Navigation
      .swiper-button-prev
        span.icon.icon-xxs.icon-circle.icon-filled-white.mdi.mdi-chevron-left.text-gray
      .swiper-button-next
        span.icon.icon-xxs.icon-circle.icon-filled-white.mdi.mdi-chevron-right.text-gray


